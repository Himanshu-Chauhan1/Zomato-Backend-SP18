{
  "name": "zomato",
  "version": "1.0.0",
  "description": "Zomato is an online and mobile restaurant search and discovery service, providing in-depth information for over 1 million restaurants across 22 countries. Zomato's core content features include menus, photos, and geocoded coordinates for restaurants. Users can rate and review restaurants, as well as create their own network of foodies.So, we are going to build backend system of zomato with NodeJS, Javascript, Postman, AWS S3, Redis, and PostgreSQL",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx nodemon app/index.js",
    "make-model": "npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,email:string",
    "down": "npx sequelize-cli db:migrate:undo",
    "migrate": "npx sequelize-cli db:migrate",
    "make-migration": "npx sequelize-cli migration:create --name modify_users_add_new_fields.",
    "revert-all-migration": "npx sequelize-cli db:migrate:undo:all",
    "revert-one-migration": "npx sequelize-cli db:migrate:undo --name 20221213045630-create-admin.js ",
    "swagger": "npx nodemon app/swagger/index.js"
  },
  "author": "Ankit Sathiya,Himanshu Chauhan",
  "license": "ISC",
